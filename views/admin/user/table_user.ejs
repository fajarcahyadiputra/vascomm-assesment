<div class="table-responsive">
    <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
        <thead>
            <tr class="text-center">
                <th>No</th>
                <th>Name</th>
                <th>Email</th>
                <th>Nomer Telepon</th>
                <th>Status</th>
                <th>Action</th>
            </tr>
        </thead>
        <tfoot>
            <tr class="text-center">
                <th>No</th>
                <th>Name</th>
                <th>Email</th>
                <th>Nomer Telepon</th>
                <th>Status</th>
                <th>Action</th>
            </tr>
        </tfoot>
        <tbody>
            <% for(let i = 0; i < dataUser.length; i++){ %>
                <tr>
                    <td class="text-center"><%= i+1 %></td>
                    <td class="text-center"><%= dataUser[i].name %></td>
                    <td class="text-center"><%= dataUser[i].email %></td>
                    <td class="text-center"><%= dataUser[i].phone_no %></td>
                    <td class="text-center"><%= dataUser[i].is_active %></td>
                    <td class="text-center">
                       
                            <button type="button" id="btn-edit" data-id="<%= dataUser[i].id %>" data-name="<%= dataUser[i].name %>" data-email="<%= dataUser[i].email %>" data-phone_no="<%= dataUser[i].phone_no %>"   class="btn btn-info btn-sm btn-circle"><i class="fa fa-edit"></i></button>
                            <button  id="btn-delete" data-id="<%= dataUser[i].id %>" class="btn btn-danger btn-sm btn-circle"><i class="fa fa-trash"></i></button>
                      
                    </td>
                </tr>
            <% } %>
        </tbody>
    </table>
</div>